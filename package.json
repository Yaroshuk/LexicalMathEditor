{
    "name": "prosemirrormath",
    "version": "0.2.1",
    "description": "Magmamath features library",
    "type": "module",
    "module": "./dist/magmamath-features.es.js",
    "types": "./dist/magmamath-features.d.ts",
    "exports": {
        ".": {
            "import": "./dist/magmamath-features.es.js",
            "types": "./dist/magmamath-features.d.ts"
        },
        "./dist/style.css": "./dist/style.css"
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build",
        "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
        "preview": "vite preview",
        "format": "prettier --write src/**/*.{ts,tsx} **/*.json",
        "lint:fix": "eslint --fix 'src/**/*.{ts,tsx}' '**/*.json'",
        "stylelint:fix": "stylelint \"src/**/*.scss\" --fix",
        "storybook": "storybook dev -p 6006",
        "build-storybook": "storybook build",
        "prepublishOnly": "yarn build",
        "chromatic": "npx chromatic --project-token=chpt_8790ae7e4d2ba98"
    },
    "lint-staged": {
        "src/**/*.{ts,tsx} **/*.json": [
            "yarn format",
            "yarn lint:fix"
        ],
        "src/**/*.scss": [
            "yarn stylelint:fix"
        ]
    },
    "publishConfig": {
        "access": "restricted"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/matteappen/magmamath-ui.git"
    },
    "license": "MIT",
    "dependencies": {
        "mathlive": "^0.98.6",
        "@lexical/react": "^0.29.0",
        "clsx": "1.2.1",
        "emoji-datasource-facebook": "^15.1.2",
        "lexical": "^0.29.0",
        "prosemirror-example-setup": "^1.2.3",
        "prosemirror-model": "^1.25.0",
        "prosemirror-schema-basic": "^1.2.4",
        "prosemirror-state": "^1.4.3",
        "prosemirror-view": "^1.39.1",
        "katex": "^0.13.2"
    },
    "devDependencies": {
        "@types/katex": "^0.11.0",
        "@types/react": "^18.0.37",
        "@types/react-dom": "^18.0.11",
        "@types/react-katex": "^3.0.4",
        "@typescript-eslint/eslint-plugin": "^5.59.0",
        "@typescript-eslint/parser": "^5.59.0",
        "@vitejs/plugin-react-swc": "^3.0.0",
        "chromatic": "^10.2.0",
        "eslint": "^8.38.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-react-refresh": "^0.3.4",
        "eslint-plugin-storybook": "^0.6.15",
        "lint-staged": "^13.2.2",
        "postcss": "8",
        "prop-types": "^15.8.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "sass": "^1.62.1",
        "scss-bundle": "3.1.2",
        "simple-git-hooks": "^2.8.1",
        "storybook": "^7.5.3",
        "stylelint": "^15.7.0",
        "stylelint-config-rational-order": "^0.1.2",
        "stylelint-config-standard-scss": "^9.0.0",
        "typescript": "^5.0.2",
        "vite": "^5.1.6",
        "vite-plugin-dts": "^2.3.0",
        "vite-plugin-lib-inject-css": "^2.1.1",
        "vite-tsconfig-paths": "^4.2.0"
    },
    "peerDependencies": {
        "effector": "^23.2.2",
        "effector-react": "^23.2.1",
        "react": ">=17.0.0",
        "react-dom": ">=17.0.0"
    },
    "engines": {
        "node": ">=14.0.0"
    },
    "resolutions": {
        "jackspeak": "2.1.1"
    }
}
